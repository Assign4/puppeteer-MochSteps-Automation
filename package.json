{
  "name": "puppeteer-mocha_automation",
  "version": "1.0.0",
  "description": "test framework with mocha steps and builder class pattern",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout=30000 --require mocha-steps ./build/tests/**.js",
    "clean" : "rmdir /s build",
    "build" : "babel --preset es2015 -d build/ src/"
  },
  "keywords": [
    "mocha",
    "puppeteer"
  ],
  "author": "manoj salugu",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "mocha-steps": "^1.3.0",
    "puppeteer": "^3.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
